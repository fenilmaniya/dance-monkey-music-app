  {
  "name": "DanceMonkey",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build-detox-android": "detox build -c android.emu.debug",
    "test-detox-android": "detox test -c android.emu.debug"
  },
  "dependencies": {
    "@nozbe/watermelondb": "^0.20.0",
    "@nozbe/with-observables": "^1.0.8",
    "@react-native-firebase/app": "^14.7.0",
    "@react-native-firebase/firestore": "^14.7.0",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.1",
    "crypto-js": "^4.1.1",
    "detox": "^19.6.2",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "react": "17.0.2",
    "react-native": "0.68.0",
    "react-native-pager-view": "^5.4.15",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-slider": "^0.11.0",
    "react-native-svg": "^12.3.0",
    "react-native-svg-icon": "^0.10.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-track-player": "^2.1.3",
    "react-redux": "^7.2.8",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/runtime": "^7.17.8",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.5.1",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "metro-react-native-babel-preset": "^0.70.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "runnerConfig": "e2e/.mocharc.json",
    "specs": "e2e/tests",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/DanceMonkey.app",
        "build": "xcodebuild -workspace ios/DanceMonkey.xcworkspace -scheme DanceMonkey -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
        "type": "iPhone 8"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/DanceMonkey.app",
        "build": "xcodebuild -workspace ios/DanceMonkey.xcworkspace -scheme DanceMonkey -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
        "type": "iPhone 8"
        },
        "artifacts": {
        "plugins": {
          "screenshot": {
          "enabled": true,
          "shouldTakeAutomaticSnapshots": true,
          "keepOnlyFailedTestsArtifacts": true,
          "takeWhen": {
            "testStart": true,
            "testDone": true,
            "appNotReady": true
          }
          }
        }
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "emu"
      }
    }
  }
}
